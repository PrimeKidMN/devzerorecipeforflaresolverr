server {
    listen 80;

    server_name too-kool.rf.gd;  # Replace with your InfinityFree domain name

    location / {
        proxy_pass http://localhost:8191;  # Replace with the port your Docker container is using
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
}
